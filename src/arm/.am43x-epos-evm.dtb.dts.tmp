  };
  partition@5 {
   label = "QSPI.KERNEL";
   reg = <0x00130000 0x0800000>;
  };
  partition@6 {
   label = "QSPI.FILESYSTEM";
   reg = <0x00930000 0x36D0000>;
  };
 };
};

&hdq {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&hdq_pins>;
};

&dss {
 status = "ok";

 pinctrl-names = "default";
 pinctrl-0 = <&dss_pins>;

 port {
  dpi_out: endpoint@0 {
   remote-endpoint = <&lcd_in>;
   data-lines = <24>;
  };
 };
};
