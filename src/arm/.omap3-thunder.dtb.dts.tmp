  compatible = "ti,omap-infra";
  mpu {
   compatible = "ti,omap3-mpu";
   ti,hwmods = "mpu";
  };

  iva: iva {
   compatible = "ti,iva2.2";
   ti,hwmods = "iva";

   dsp {
    compatible = "ti,omap3-c64";
   };
  };
 };
