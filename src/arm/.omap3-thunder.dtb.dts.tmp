/ {
 compatible = "ti,omap3430", "ti,omap3";
 interrupt-parent = <&intc>;

 aliases {
  i2c0 = &i2c1;
  i2c1 = &i2c2;
  i2c2 = &i2c3;
  serial0 = &uart1;
  serial1 = &uart2;
  serial2 = &uart3;
 };

 cpus {
  #address-cells = <1>;
  #size-cells = <0>;

  cpu@0 {
   compatible = "arm,cortex-a8";
   device_type = "cpu";
   reg = <0x0>;

   clocks = <&dpll1_ck>;
   clock-names = "cpu";

   clock-latency = <300000>;
  };
 };

 pmu {
  compatible = "arm,cortex-a8-pmu";
  reg = <0x54000000 0x800000>;
  interrupts = <3>;
  ti,hwmods = "debugss";
 };





