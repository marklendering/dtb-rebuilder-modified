   syscon = <&omap3_scm_general>;
   pbias_mmc_reg: pbias_mmc_omap2430 {
    regulator-name = "pbias_mmc_omap2430";
    regulator-min-microvolt = <1800000>;
    regulator-max-microvolt = <3000000>;
